{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Tayaba Web Debug",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
      "args": ["dev"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "protocol": "inspector",
      "env": {
        "PORT": "3050"
      },
      "runtimeArgs": ["--inspect-brk"]
    }
  ]
}
